,QAAA,IAAA,CAAA,IAAA,GAAA,QAAA,CAAA,IAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,YAAA,CAAA,OAAA,CAAA,OAAA,CAAA;AACA,OAHA;AAIA,KAfA;AAgBA,IAAA,eAhBA,2BAgBA,GAhBA,EAgBA;AACA,UAAA,CAAA,GAAA,CAAA,IAAA,CAAA,KAAA,EAAA;AACA,aAAA,WAAA;AACA;AACA;;AAEA,MAAA,YAAA,CAAA,KAAA,GAAA,GAAA,CAAA,IAAA,CAAA,KAAA;AACA,WAAA,KAAA,GAAA,KAAA;AAEA,WAAA,OAAA,CAAA,OAAA,CAAA,KAAA,MAAA,CAAA,KAAA,CAAA,QAAA,IAAA,2BAAA,KAAA,MAAA,CAAA,MAAA,CAAA,IAAA;AACA,KA1BA;AA2BA,IAAA,WA3BA,yBA2BA;AACA,WAAA,KAAA,GAAA,eAAA;AACA,KA7BA;AA8BA,IAAA,OA9BA,qBA8BA;AACA,WAAA,iBAAA;AACA,KAhCA;AAiCA,IAAA,iBAjCA,+BAiCA;AACA,UAAA,YAAA,CAAA,KAAA,EAAA;AACA,aAAA,OAAA,CAAA,OAAA,CAAA,KAAA,MAAA,CAAA,KAAA,CAAA,QAAA,IAAA,2BAAA,KAAA,MAAA,CAAA,MAAA,CAAA,IAAA;AACA;AACA;AArCA;AApBA,CAAA","sourcesContent":["<template>\r\n      <section class=\"login\">\r\n              <div class=\"card mx-xl-5\">\r\n                <div class=\"card-body\">\r\n                  <div v-if=\"name\">\r\n                  <mdb-list-group>\r\n                    <mdb-list-group-item>Order Number : {{ name.order[0].shipname }}</mdb-list-group-item>\r\n                    <mdb-list-group-item>Order Date : {{ name.order[0].created_at }}</mdb-list-group-item>\r\n                    <mdb-list-group-item>Merchant Name : {{ name.order[0].merchant_cred }} </mdb-list-group-item>\r\n                    <mdb-list-group-item>Order Amount : {{ parseFloat(name.order[0].total_amount).toFixed(2)}}</mdb-list-group-item>\r\n                  </mdb-list-group>       \r\n                  </div>           \r\n                  <form @submit.prevent=\"login\">\r\n                    <p class=\"h4 text-center py-4\">Please login to continue the payment</p>   \r\n                    <mdb-alert color=\"danger\" v-if=\"error\">\r\n                      {{ error }}\r\n                    </mdb-alert>           \r\n                    <label htmlFor=\"defaultFormCardNameEx\" class=\"grey-text font-weight-light\">Your email</label>\r\n                    <input v-model=\"email\" type=\"text\" id=\"defaultEmailCardNameEx\" class=\"form-control\" required/>\r\n                    <br />\r\n                    <label htmlFor=\"defaultFormCardEmailEx\" class=\"grey-text font-weight-light\">Your password</label>\r\n                    <input v-model=\"password\" type=\"password\" id=\"defaultFormPasswordEx\" class=\"form-control\" required/>\r\n                    <div class=\"text-center py-4 mt-3\">\r\n                      <button class=\"btn btn-indigo\" type=\"submit\" >Login</button>\r\n                    </div>\r\n                  </form>\r\n                </div>\r\n              </div>\r\n      </section>\r\n</template>\r\n\r\n<script>\r\n\r\nimport { mdbAlert, mdbListGroup, mdbListGroupItem } from 'mdbvue';\r\nimport axios from 'axios'\r\n\r\nexport default {\r\n  name: 'login_view',\r\n  data () {\r\n    return {\r\n      id: this.$route.params.hash,\r\n      date: name.shipname,\r\n      name: null,\r\n      email: '',\r\n      password: '',\r\n      error: false\r\n    }\r\n  },\r\n  mounted : function() {\r\n    this.getData();\r\n  },\r\n  components: {\r\n    mdbAlert,\r\n    mdbListGroup,\r\n    mdbListGroupItem    \r\n  },  \r\n  methods: {\r\n    login () {\r\n            let email = this.email;\r\n            let password = this.password;\r\n            this.$store\r\n              .dispatch(\"login\", { email, password })\r\n              .then(request => this.loginSuccessful(request))\r\n              .catch(() => this.loginFailed())\r\n  },\r\n    getData() {\r\n      var that = this\r\n     axios.get('http://localhost:9000/api/order/'+ this.$route.params.hash).then(function(response) {\r\n     that.name = response.data\r\n     console.log(localStorage.getItem('token'));\r\n    })\r\n    },\r\n    loginSuccessful (req) {\r\n     if (!req.data.token) {\r\n       this.loginFailed()\r\n       return\r\n     }\r\n   \r\n     localStorage.token = req.data.token\r\n     this.error = false\r\n   \r\n     this.$router.replace(this.$route.query.redirect || '/payment_confirmation/'+ this.$route.params.hash)\r\n   },  \r\n   loginFailed () {\r\n     this.error = 'Login failed!'\r\n   },\r\n   updated () {\r\n     this.checkCurrentLogin()\r\n   },\r\n   checkCurrentLogin () {\r\n     if (localStorage.token) {\r\n       this.$router.replace(this.$route.query.redirect || '/payment_confirmation/'+ this.$route.params.hash)\r\n     }\r\n   },\r\n }\r\n}\r\n\r\n</script>\r\n\r\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\r\n<style scoped>\r\nh3 {\r\n  font-weight: normal;\r\n  padding-top: 20px;\r\n  padding-bottom: 30px;\r\n}\r\np {\r\n  color: #969696;\r\n  margin-bottom: 0;\r\n  font-size: 14px;\r\n}\r\n\r\n</style>\r\n"],"sourceRoot":"src/components"}]}