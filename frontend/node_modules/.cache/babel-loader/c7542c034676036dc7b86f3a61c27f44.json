xt\" id=\"defaultTelephoneCardNameEx\" class=\"form-control\" required/>\r\n                    <br />\r\n                    <input v-model=\"merchant_id\" type=\"hidden\" id=\"defaultMerchantCardNameEx\" class=\"form-control\" value=\"23123123\" >\r\n                    <input v-model=\"shipcountry\" type=\"hidden\" id=\"defaultshipcountryCardNameEx\" class=\"form-control\" value=\"MY\" >\r\n                    <input v-model=\"api_key\" type=\"hidden\" id=\"defaultapikeyCardNameEx\" class=\"form-control\" value=\"e368ed39-c033-4cde-8007-438265e1d7e5\" >\r\n                    <div class=\"text-center py-4 mt-3\">\r\n                      <button class=\"btn btn-indigo\" type=\"submit\" >Order</button>\r\n                    </div>\r\n                  </form>\r\n                </div>\r\n              </div>\r\n      </section>\r\n</template>\r\n\r\n\r\n<script>\r\n\r\nimport { mdbAlert } from 'mdbvue';\r\n\r\nexport default {\r\n  name: 'login_view',\r\n  data () {\r\n    return {\r\n      order_id: '',\r\n      amount: '',\r\n      email: '',\r\n      telephone: '',\r\n      error: false\r\n    }\r\n  },\r\n  components: {\r\n    mdbAlert   \r\n  },  \r\n  methods: {\r\n    form () {\r\n            let shipname = this.order_id;\r\n            let total_amount = this.amount;\r\n            let email = this.email;\r\n            let telephone = this.telephone;\r\n            let merchant_cred = this.merchant_id;\r\n            let shipcountry = this.shipcountry;\r\n            let api_key = this.api_key;\r\n            this.$store\r\n              .dispatch(\"form\", { shipname, email, total_amount, shipcountry, telephone, merchant_cred, api_key})\r\n              .then(request => this.loginSuccessful(request))\r\n              .catch(() => this.loginFailed())\r\n\r\n    },\r\n   loginSuccessful (req) {\r\n     if (!req.data.token) {\r\n       this.loginFailed()\r\n       return\r\n     }\r\n   \r\n     localStorage.token = req.data.token\r\n     this.error = false\r\n   \r\n     this.$router.replace(this.$route.query.redirect || '/payment_confirmation')\r\n   },  \r\n   loginFailed () {\r\n     this.error = 'Login failed!'\r\n     delete localStorage.token\r\n   },\r\n   created () {\r\n     this.checkCurrentLogin()\r\n   },\r\n   updated () {\r\n     this.checkCurrentLogin()\r\n   },\r\n   checkCurrentLogin () {\r\n     if (localStorage.token) {\r\n       this.$router.replace(this.$route.query.redirect || '/payment_confirmation')\r\n     }\r\n   },\r\n }\r\n}\r\n\r\n</script>\r\n\r\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\r\n<style scoped>\r\nh3 {\r\n  font-weight: normal;\r\n  padding-top: 20px;\r\n  padding-bottom: 30px;\r\n}\r\np {\r\n  color: #969696;\r\n  margin-bottom: 0;\r\n  font-size: 14px;\r\n}\r\n\r\n</style>\r\n"],"sourceRoot":"src/components"}]}